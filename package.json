{
  "name": "payload-playground",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.6.1+sha512.40ee09af407fa9fbb5fbfb8e1cb40fbb74c0af0c3e10e9224d7b53c7658528615b2c92450e74cfad91e3a2dcafe3ce4050d80bda71d757756d2ce2b66213e9a3",
  "description": "Website template for Payload",
  "license": "MIT",
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "scripts": {
    "clean": "rm -rf .next",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm db:up && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "typecheck": "cross-env NODE_OPTIONS=--no-deprecation tsc --noEmit",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation payload migrate && next start",
    "test": "vitest",
    "test:run": "pnpm test run",
    "prepare": "pnpm dlx husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "^3.27.0",
    "@payloadcms/email-nodemailer": "^3.27.0",
    "@payloadcms/email-resend": "^3.27.0",
    "@payloadcms/live-preview-react": "^3.27.0",
    "@payloadcms/next": "^3.27.0",
    "@payloadcms/payload-cloud": "^3.27.0",
    "@payloadcms/plugin-cloud-storage": "^3.27.0",
    "@payloadcms/plugin-form-builder": "^3.27.0",
    "@payloadcms/plugin-nested-docs": "^3.27.0",
    "@payloadcms/plugin-redirects": "^3.27.0",
    "@payloadcms/plugin-search": "^3.27.0",
    "@payloadcms/plugin-seo": "^3.27.0",
    "@payloadcms/richtext-lexical": "^3.27.0",
    "@payloadcms/storage-s3": "^3.27.0",
    "@payloadcms/ui": "^3.27.0",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "geist": "^1.3.1",
    "graphql": "^16.10.0",
    "lucide-react": "^0.378.0",
    "next": "^15.2.1",
    "next-sitemap": "^4.2.3",
    "nodemailer-sendgrid": "^1.0.3",
    "payload": "latest",
    "payload-admin-bar": "^1.0.7",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "7.45.4",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.16.0",
    "@chromatic-com/storybook": "^3.2.5",
    "@eslint-react/eslint-plugin": "^1.31.0",
    "@eslint/eslintrc": "^3.3.0",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/addon-onboarding": "^8.6.4",
    "@storybook/blocks": "^8.6.4",
    "@storybook/experimental-addon-test": "^8.6.4",
    "@storybook/experimental-nextjs-vite": "8.6.2",
    "@storybook/react": "^8.6.4",
    "@storybook/test": "^8.6.4",
    "@tailwindcss/postcss": "^4.0.12",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/escape-html": "^1.0.4",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "22.5.4",
    "@types/nodemailer-sendgrid": "^1.0.3",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "3.0.7",
    "@vitest/coverage-v8": "^3.0.8",
    "copyfiles": "^2.4.1",
    "eslint": "^9.21.0",
    "eslint-config-next": "15.1.5",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^0.11.4",
    "husky": "^9.1.7",
    "jest-axe": "^9.0.0",
    "jsdom": "^26.0.0",
    "playwright": "^1.51.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "storybook": "^8.6.4",
    "tailwindcss": "^4.0.12",
    "typescript": "5.7.3",
    "vitest": "^3.0.8"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "msw",
      "sharp"
    ],
    "supportedArchitectures": {
      "os": [
        "darwin",
        "current"
      ],
      "cpu": [
        "x64",
        "arm64"
      ]
    }
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
