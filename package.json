    "graphql": "^16.10.0",
    "lucide-react": "^0.484.0",
    "next": "^15.2.4",
    "next-sitemap": "^4.2.3",
    "nodemailer-sendgrid": "^1.0.3",
    "payload": "^3.31.0",
    "payload-admin-bar": "^1.0.7",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.6.0",
