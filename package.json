    "eslint-config-next": "15.1.5",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^0.11.6",
    "husky": "^9.1.7",
    "jest-axe": "^9.0.0",
    "jsdom": "^26.0.0",
    "playwright": "^1.51.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "storybook": "^8.6.9",
    "tailwindcss": "^4.0.17",
    "typescript": "5.7.3",
    "vitest": "^3.0.9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js-pure",
      "esbuild",
      "msw",
      "sharp"
    ],
    "supportedArchitectures": {
      "os": [
        "darwin",
        "current"
      ],
      "cpu": [
        "x64",
        "arm64"
      ]
    }
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
